{
    "name": "Dark/Light.io",
    "description" : "Alternate between Dark and Light Mode",
    "version" : "1.0.0",
    "manifest_version":2,
    "background":{
        "scripts": [ "popup.js"],
        "persisten" :true
    },
    "browser_action" : {
        "default_title": "Dark/Light.io",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}